{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["node", "@playwright/test", "jest"]
  },
  "include": [
    "js/",
    "BACKEND/",
    "TESTS/"
  ],
  "exclude": [
    "node_modules"
  ]
}
